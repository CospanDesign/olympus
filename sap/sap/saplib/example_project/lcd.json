{
	
	"BASE_DIR":"~/projects/sycamore_projects",
	"PROJECT_NAME":"example_project",
	"BUILD_TOOL":"xilinx",
	"TEMPLATE":"wishbone_template.json",
	"INTERFACE":"uart_io_handler.v",
	"SLAVES":{
		"lcd_panel":{
			"filename":"tft.v"
		}
	},

	"CONSTRAINTS":{
		"constraint_files":[
			"s3esk_tft.ucf"
		],
		"board":"xilinx-s3esk",
		"device":"xc3s500efg320",
		"bind": {
			"lcd_panel_red[7:0]":{
				"port":"tft_red[7:0]",
				"direction":"output"
			},
			"lcd_panel_green[7:0]":{
				"port":"tft_green[7:0]",
				"direction":"output"
			},
			"lcd_panel_blue[7:0]":{
				"port":"tft_blue[7:0]",
				"direction":"output"
			},
			"lcd_panel_disp_en":{
				"port":"tft_disp_en",
				"direction":"output"
			},
			"lcd_panel_hsync":{
				"port":"tft_hsync",
				"direction":"output"
			},
			"lcd_panel_vsync":{
				"port":"tft_vsync",
				"direction":"output"
			},
			"lcd_panel_data_en":{
				"port":"tft_data_en",
				"direction":"output"
			},
			"lcd_panel_pclk":{
				"port":"tft_pclk",
				"direction":"output"
			},
			"phy_uart_in":{
				"port":"rx",
				"direction":"input"
			},
			"phy_uart_out":{
				"port":"tx",
				"direction":"output"
			}
		}
	}
}
