{
	
	"BASE_DIR":"~/projects/sycamore_projects",
	"PROJECT_NAME":"example_project",
	"BUILD_TOOL":"xilinx",
	"TEMPLATE":"wishbone_template.json",
	"INTERFACE":{
		"filename":"uart_io_handler.v"
	},
	"SLAVES":{
		"gpio1":{
			"filename":"wb_gpio.v"
		}
	},

	"CONSTRAINTS":{
		"constraint_files":[
			"oak.ucf"
		],
		"board":"oak",
		"device":"xa3s250epqg208-4",
		"bind": {
			"phy_uart_in":{
				"port":"gpio_rx",
				"direction":"input"
			},
			"phy_uart_out":{
				"port":"gpio_tx",
				"direction":"output"
			},
			"gpio1_gpio_out[0]":{
				"port":"led",
				"direction":"output"
			}
		}
	}
}
