{
	
	"BASE_DIR":"~/projects/sycamore_projects",
	"PROJECT_NAME":"example_project",
	"BUILD_TOOL":"xilinx",
	"TEMPLATE":"wishbone_template.json",
	"INTERFACE":"uart_io_handler.v",
	"SLAVES":[
		"tft.v"
	],

	"CONSTRAINTS":{
		"constraint_files":[
			"s3esk_tft.ucf"
		],
		"device":"xc3s500efg320",
		"bind": {
			"s1_red[7:0]":{
				"port":"tft_red[7:0]",
				"direction":"output"
			},
			"s1_green[7:0]":{
				"port":"tft_green[7:0]",
				"direction":"output"
			},
			"s1_blue[7:0]":{
				"port":"tft_blue[7:0]",
				"direction":"output"
			},
			"s1_disp_en":{
				"port":"tft_disp_en",
				"direction":"output"
			},
			"s1_hsync":{
				"port":"tft_hsync",
				"direction":"output"
			},
			"s1_vsync":{
				"port":"tft_vsync",
				"direction":"output"
			},
			"s1_data_en":{
				"port":"tft_data_en",
				"direction":"output"
			},
			"s1_pclk":{
				"port":"tft_pclk",
				"direction":"output"
			},
			"phy_uart_in":{
				"port":"rx",
				"direction":"input"
			},
			"phy_uart_out":{
				"port":"tx",
				"direction":"output"
			}
		}
	}
}
